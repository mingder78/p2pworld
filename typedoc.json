{
  "name": "p2pworld",
  "entryPoints": ["src/*.ts"],
  "out": "docs",
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeProtected": true,
  "readme": "./README.md",
  "includeVersion": true,
  "exclude": ["**/node_modules/**", "**/dist/**", "**/test/**", "**/*.test.ts", "**/*.spec.ts"],
  "headings": {
    "readme": false,
    "document": false
  }
}

